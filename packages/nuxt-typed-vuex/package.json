  "dependencies": {
    "typed-vuex": "0.2.0",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "core-js": "3.19.1",
    "nuxt": "^2.15.8",
    "ts-loader": "^8.3.0",
    "tsd": "^0.18.0"
  },
  "peerDependencies": {
    "@nuxt/types": "^2.15.8"
  },
  "tsd": {
    "directory": "test/tsd",
    "compilerOptions": {
      "rootDir": "."
    }
  }
